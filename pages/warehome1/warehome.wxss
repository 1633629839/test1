/* pages/warehome/warehome.wxss */ /* */
.warehomeItem{
  height: 100px;
  border-bottom: 5px solid red;/* 下边框 宽5px 实线 红色*/
  display: flex;
}

.warehomeItem .fm{
  width: 140px;/*宽度1400px */
  height: 80px;/*高度80px */
  background-color: orange;/*背景颜色橙色*/
  margin: 10px;/* 外边框宽度 10px*/
}

.warehomeItem .dese{
  flex: 1;/*把多余的宽度给它 */
  background-color: cyan;/*背景颜色浅绿色 */
  padding: 10px;/* 内边框10px  内容会距离盒子边框10px*/
}